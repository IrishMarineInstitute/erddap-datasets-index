<dataset type="EDDTableFromNcCFFiles" datasetID="datasetsIndex" active="true">
    <reloadEveryNMinutes>10080</reloadEveryNMinutes>
    <updateEveryNMillis>10000</updateEveryNMillis>
    <fileDir>/opt/data/index</fileDir>
    <fileNameRegex>.*\.nc</fileNameRegex>
    <recursive>true</recursive>
    <pathRegex>.*</pathRegex>
    <metadataFrom>last</metadataFrom>
    <preExtractRegex></preExtractRegex>
    <postExtractRegex></postExtractRegex>
    <extractRegex></extractRegex>
    <columnNameForExtract></columnNameForExtract>
    <sortFilesBySourceNames></sortFilesBySourceNames>
    <fileTableInMemory>false</fileTableInMemory>
    <accessibleViaFiles>false</accessibleViaFiles>
    <addAttributes>
        <att name="cdm_data_type">Point</att>
        <att name="Conventions">COARDS, CF-1.6, ACDD-1.3</att>
        <att name="creator_email">datarequests@marine.ie</att>
        <att name="creator_name">Marine Institute</att>
        <att name="creator_url">https://www.marine.ie</att>
        <att name="date_created">2020-08-10</att>
        <att name="date_issued">2020-08-10</att>
        <att name="date_modified">2020-08-10</att>
        <att name="Easternmost_Easting" type="float">0.0</att>
        <att name="featureType">Point</att>
        <att name="geospatial_lat_max" type="float">0.0</att>
        <att name="geospatial_lat_min" type="float">0.0</att>
        <att name="geospatial_lat_units">degrees_north</att>
        <att name="geospatial_lon_max" type="float">0.0</att>
        <att name="geospatial_lon_min" type="float">0.0</att>
        <att name="geospatial_lon_units">degrees_east</att>
        <att name="id">waveatlantos00</att>
        <att name="infoUrl">https://www.marine.ie/</att>
        <att name="institution">Irish Marine Institute</att>
        <att name="keywords_vocabulary">GCMD Science Keywords</att>
        <att name="license">Creative Commons Attribution 4.0 (https://creativecommons.org/licenses/by/4.0/)</att>
        <att name="licenseUrl">https://creativecommons.org/licenses/by/4.0/legalcode</att>
        <att name="Northernmost_Northing" type="float">0.0</att>
        <att name="publisher_email">datarequests@marine.ie</att>
        <att name="publisher_name">Marine Institute</att>
        <att name="publisher_url">http://www.marine.ie</att>
        <att name="sdn_edmo_code">396</att>
        <att name="sourceUrl">(source database)</att>
        <att name="Southernmost_Northing" type="float">0.0</att>
        <att name="standard_name_vocabulary">CF Standard Name Table v29</att>
        <att name="subsetVariables">dataset_id,identifier,year,month</att>
        <att name="time_coverage_end">2020-08-16T00:00:00Z</att>
        <att name="time_coverage_start">2020-08-04T00:00:00Z</att>
        <att name="title">Geospatial Index of ERDDAP Dataset Records</att>
        <att name="Westernmost_Easting" type="float">0.0</att>
        <att name="creator_type">person</att>
        <att name="keywords">latitude, longitude, elevation, time</att>
        <att name="publisher_type">person</att>
        <att name="sourceUrl">(local files)</att>
        <att name="summary">Index for dataset discovery. Irish Marine Institute data from a local source.</att>
        <att name="testOutOfDate">now+4days</att>
    </addAttributes>
    <dataVariable>
        <sourceName>latitude</sourceName>
        <destinationName>latitude</destinationName>
        <dataType>float</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="_CoordinateAxisType">Lat</att>
            <att name="axis">Y</att>
            <att name="colorBarMaximum" type="float">90.0</att>
            <att name="colorBarMinimum" type="float">-90.0</att>
            <att name="ioos_category">Location</att>
            <att name="long_name">Latitude</att>
            <att name="standard_name">latitude</att>
            <att name="units">degrees_north</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>longitude</sourceName>
        <destinationName>longitude</destinationName>
        <dataType>float</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="_CoordinateAxisType">Lon</att>
            <att name="axis">X</att>
            <att name="colorBarMaximum" type="float">180.0</att>
            <att name="colorBarMinimum" type="float">-180.0</att>
            <att name="ioos_category">Location</att>
            <att name="long_name">Longitude</att>
            <att name="standard_name">longitude</att>
            <att name="units">degrees_east</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>elevation</sourceName>
        <destinationName>elevation</destinationName>
        <dataType>int</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="_CoordinateAxisType">Height</att>
            <att name="axis">Z</att>
            <att name="colorBarMaximum" type="float">8000</att>
            <att name="colorBarMinimum" type="float">-8000</att>
            <att name="ioos_category">Location</att>
            <att name="long_name">Elevation relative to sea level</att>
            <att name="standard_name">height_above_reference_ellipsoid</att>
            <att name="units">m</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>time</sourceName>
        <destinationName>time</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="_CoordinateAxisType">Time</att>
            <att name="axis">T</att>
            <att name="ioos_category">Time</att>
            <att name="long_name">Time Interval</att>
            <att name="standard_name">time</att>
            <att name="time_origin">01-JAN-1970 00:00:00</att>
            <att name="units">seconds since 1970-01-01T00:00:00Z</att>
            <att name="colorBarMaximum" type="double">1.598E9</att>
            <att name="colorBarMinimum" type="double">1.596E9</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>year</sourceName>
        <destinationName>year</destinationName>
        <dataType>int</dataType>
        <addAttributes>
            <att name="coordinates">time latitude longitude</att>
            <att name="ioos_category">Time</att>
            <att name="long_name">Year</att>
            <att name="colorBarMaximum" type="double">2150.0</att>
            <att name="colorBarMinimum" type="double">1900.0</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>month</sourceName>
        <destinationName>month</destinationName>
        <dataType>int</dataType>
        <addAttributes>
            <att name="coordinates">time latitude longitude</att>
            <att name="ioos_category">Time</att>
            <att name="long_name">Month</att>
            <att name="colorBarMaximum" type="int">12</att>
            <att name="colorBarMinimum" type="int">1</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>dataset_id</sourceName>
        <destinationName>dataset_id</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="comment">The id of ERDDAP dataset to which the row belongs</att>
            <att name="coordinates">time latitude longitude</att>
            <att name="ioos_category">Identifier</att>
            <att name="long_name">Dataset ID</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>identifier</sourceName>
        <destinationName>identifier</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="comment">The code used to identify the device/platform/station in ERDDAP</att>
            <att name="coordinates">time latitude longitude</att>
            <att name="ioos_category">Identifier</att>
            <att name="long_name">Identifier</att>
        </addAttributes>
    </dataVariable>
</dataset>
