            <att name="sdn_p01_urn">SDN:P01::ALONZZ01</att>
            <att name="sdn_p06_urn">SDN:P06::UAAA</att>
            <att name="standard_name">longitude</att>
            <att name="units">degrees_east</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>time</sourceName>
        <destinationName>time</destinationName>
        <dataType>double</dataType>
        <!-- sourceAttributes>
        </sourceAttributes -->
        <addAttributes>
            <att name="_CoordinateAxisType">Time</att>
            <att name="axis">T</att>
            <att name="ioos_category">Time</att>
            <att name="long_name">Time Interval</att>
            <att name="sdn_p01_label">Date (time from 00:00 01/01/1760 to 00:00 UT on day)</att>
